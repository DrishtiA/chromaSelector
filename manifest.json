{
    "manifest_version": 3,
    "name": "color picker",
    "description": "a color picking extension",
    "version": "1.0",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "icons": {
        "16": "/image/picker_16.png",
        "32": "/image/picker_32.png",
        "48": "/image/picker_48.png"
    }
}
